{
    "workbench.startupEditor": "newUntitledFile",
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "editor.occurrencesHighlight": true,
    "editor.selectionHighlight": true,
    "editor.renderWhitespace": "all",
    "editor.formatOnPaste": true,
    "git.autofetch": false,
    "extensions.ignoreRecommendations": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.autoGuessEncoding": true,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 14,
    "workbench.editor.tabSizing": "shrink",
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.tabCompletion": "on",
    "editor.dragAndDrop": false,
    "explorer.confirmDragAndDrop": false,
    "editor.snippetSuggestions": "top",
    "explorer.confirmDelete": false,
    "githd.blameView.enabled": false,
    "diffEditor.renderSideBySide": false,
    "git.confirmSync": false,
    "editor.wrappingIndent": "indent",
    "[vbnet]": {
        "editor.tabSize": 4
    },
    "[python]": {
        "editor.tabSize": 4
    },
    "[c]": {
        "editor.tabSize": 4
    },
    "[cpp]": {
        "editor.tabSize": 4
    },
    "python.pythonPath": "/usr/local/bin/python3",
    "csv-edit.fontSizeInPx": 13,
    "csv-edit.doubleClickColumnHandleForcedWith": 10,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.renderCharacters": false,
    "indenticator.showHover": true,
    "indenticator.showCurrentDepthInStatusBar": false,
    "indenticator.style": "dashed",
    "codemap.vb": [
        {
            "pattern": "^\\s*Sub \\w*",
            "clear": "Sub ",
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Private Sub \\w*",
            "clear": "Private Sub ",
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Public Sub \\w*",
            "clear": "Public Sub" ,
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Protected Sub \\w*",
            "clear": "Protected Sub" ,
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Function \\w*",
            "clear": "Function ",
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Private Function \\w*",
            "clear": "Private Function ",
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Public Function \\w*",
            "clear": "Public Function" ,
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Protected Function \\w*",
            "clear": "Protected Function" ,
            "suffix": "(...)",
            "icon": "function"
        },
        {
            "pattern": "^\\s*Public Class \\w*",
            "clear": "Public Class ",
            "icon": "class"
        },
        {
            "pattern": "^\\s*Module \\w*",
            "clear": "Module ",
            "icon": "class"
        },
        {
            "pattern": "^\\s*Private m_\\w*",
            "clear": "Private ",
            "icon": "property"
        },
        {
            "pattern": "^#Region .+\\w*",
            "clear": "#Region ",
            "icon": "none"
        }
    ],
    "codemap.cmd": [
        {
            "pattern": "^:L_.*",
            "icon": "none"
        }
    ],
    "python.languageServer": "Microsoft",
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "window.zoomLevel": -0.4,
    "debug.console.fontSize": 12.5,
    "editor.fontSize": 12.5,
    "editor.lineHeight": 15,
    "editor.fontFamily": "Source Han Code JP, Consolas, Meiryo, 'Courier New', monospace"
}
